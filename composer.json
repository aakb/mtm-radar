{
    "name": "aakb/mtm-radar",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.0.0",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^1.17",
        "egeloen/ckeditor-bundle": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "incenteev/composer-parameter-handler": "^2.1",
        "itk-dev/config-bundle": "^1.0",
        "itk-dev/database-bundle": "^1.1",
        "itk/easyadmin-user-bundle": "dev-feature/RADAR-17-admin-security",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.1",
        "squizlabs/php_codesniffer": "^3.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/polyfill-apcu": "^1.7",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/symfony": "3.4.*",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Ivory\\CKEditorBundle\\Composer\\CKEditorScriptHandler::install",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "check-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix --dry-run"
        ],
        "check-coding-standards/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "check-coding-standards/twigcs": [
            "vendor/bin/twigcs lint app/Resources/views"
        ],
        "check-coding-standards": [
            "@check-coding-standards/php-cs-fixer",
            "@check-coding-standards/phpcs"
        ],
        "fix-coding-standards/php-cs-fixer": [
            "php -n vendor/bin/php-cs-fixer --config=.php_cs.dist fix"
        ],
        "fix-coding-standards/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "fix-coding-standards": [
            "@fix-coding-standards/php-cs-fixer",
            "@fix-coding-standards/phpcs"
        ],
        "install-git-hooks": [
            "ln -sf ../../githooks/pre-commit .git/hooks"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null,
        "ckeditor-tag": "4.8.0",
        "ckeditor-clear": "drop"
    },
    "repositories": {
        "itk_easyadmin_user_bundle": {
            "type": "path",
            "url": "bundles/itk/easyadmin-user-bundle"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
